{
    "name": "silverstripeltd/discoverer-theme",
    "description": "The Silverstripe search service starter theme",
    "type": "silverstripe-vendormodule",
    "license": "proprietary",
    "homepage": "https://github.com/silverstripeltd/discoverer-theme",
    "keywords": [
        "silverstripe",
        "discoverer",
        "search",
        "theme"
    ],
    "require": {
        "silverstripe/framework": "^5",
        "silverstripeltd/discoverer": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "php-http/discovery": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "installer-name": "discoverer-theme",
        "expose": [
            "dist"
        ]
    }
}
